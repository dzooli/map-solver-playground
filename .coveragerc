[run]
source = src\map_solver_playground
omit = 
    *\tests\*
    *\__pycache__\*
    *\__init__.py
    *\setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    def __str__

[html]
directory = htmlcov

[lcov]
output = lcov.info
